.table {
  border: 0px solid darkgray;
  margin-top: 100rpx;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 4rem;
  align-items: center;
}
.td {
    width:50%;
    justify-content: center;
    text-align: center;
    color: #555555;
    font-size: 50rpx;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #f5f2f2;
}
.th {
  width: 50%;
  justify-content: center;
  background: #999999;
  color: #fff;
  font-size: 50rpx;
  display: flex;
  height: 4rem;
  align-items: center;
}