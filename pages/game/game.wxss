page {
  align-items: center;
}

.container {
  border: 15rpx solid #f5f2f2;
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-between;
  border-radius: 20rpx;
  margin-top: 40rpx;
}

.grid {
  width: 150rpx;
  height: 150rpx;
  font-size: 50rpx;
  font-weight: 600;
  position: relative;
}

.grid view {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 10;
}

.empty {
  width: 150rpx;
  height: 150rpx;
  background: #fff;
}

.text {
  font-size: 32rpx;
  margin-top: 40rpx;
  color: #555555;
}

.button {
  width: 400rpx;
  height: 80rpx;
  background:  #f5f2f2;
  color: #555555;
  border-radius: 10rpx;
  font-size: 36rpx;
  margin-top: 40rpx;
}